{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "Debug"
    },
    "console": {
      "isEnabled": true
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "functions": [
    "my_function"
  ],
  "extensions": {
    "http": {
      "routePrefix": "api"
    }
  },
  "healthMonitor": {
    "enabled": false
  }
}
